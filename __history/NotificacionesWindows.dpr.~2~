program NotificacionesWindows;

uses
  Vcl.Forms,
  UnidadNotificacion in 'UnidadNotificacion.pas' {NotificacionesWindows};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
